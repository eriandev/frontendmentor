{
  "name": "frontendmentor",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "preview": "astro preview",
    "build": "pnpm check && astro build",
    "check": "astro check --tsconfig tsconfig.json",
    "lint": "eslint --fix {apps,packages,src}/**/*.{astro,ts,js}",
    "format": "prettier --write {apps,packages,src}/**/*.{astro,ts,js,css}",
    "all:run": "pnpm -r --stream --workspace-concurrency 3",
    "build:prod": "pnpm build && pnpm all:run build"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "dependencies": {
    "astro": "5.12.8"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@types/node": "24.2.0",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "eslint": "9.32.0",
    "eslint-config-love": "121.0.0",
    "eslint-plugin-astro": "1.3.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.14",
    "shared": "workspace:*",
    "sharp": "0.34.3",
    "typescript": "5.8.3",
    "vite": "6.3.5"
  }
}