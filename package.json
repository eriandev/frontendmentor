{
  "name": "frontendmentor",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "astro": "astro",
    "dev": "astro dev",
    "preview": "astro preview",
    "build": "pnpm check && astro build",
    "check": "astro check --tsconfig tsconfig.json",
    "lint": "eslint --fix {src,apps,packages}/**/*.{astro,ts,js}",
    "format": "prettier --write {src,apps,packages}/**/*.{astro,ts,js,css}",
    "all:run": "pnpm -r --stream --workspace-concurrency 3",
    "build:prod": "pnpm build && pnpm all:run build"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=8"
  },
  "dependencies": {
    "astro": "5.5.5"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@types/node": "22.13.13",
    "eslint": "9.23.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "shared": "workspace:*",
    "sharp": "0.33.5",
    "typescript": "5.8.2",
    "vite": "6.2.3"
  }
}